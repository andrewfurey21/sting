
var i = 2;
var max = 100;
while (i <= max) {
    var start = 2;
    var is_prime = true;
    var factor = 0;

    while (start < i and is_prime) {
    	var result = i;

	while (result > 0) {
	    result = result - start;
	}

	if (result + 0.0001 > 0) {
	    is_prime = false;
	    factor = start;
	}
	start = start + 1;
    }

    if (is_prime) {
        print i;
    }

    i = i + 1;
}
