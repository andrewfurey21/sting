

var i = 2;
while (i <= 2003) {
    var start = 2;
    var is_prime = true;
    var factor = 0;

    while (start < i and is_prime) {
    	var result = i;

	while (result > 0) {
	    result = result - start;
	}

	if (result + 0.0001 > 0) {
	    is_prime = false;
	    factor = start;
	}
	start = start + 1;
    }

    print i;
    if (is_prime) {
        print " is prime!";
    } else {
        print " is divisible by: ";
        print factor;
    }

    i = i + 1;
}
