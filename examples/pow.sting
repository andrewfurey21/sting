fun pow(n, m) {
    if (m < 0) return nil;
    var result = 1;
    for (var i = 0; i < m; i = i + 1) {
	result = result * n;
    }
    return result;
}

for (var i = -1; i < 10; i = i + 1) {
    var result = pow(2, i);
    if (result != nil) {
	print result;
    } else {
	print "Cannot pow negative numbers";
    }
}

